{
    "version": 2,
    "buildCommand": "npm install -f && npm run build",
    "outputDirectory": "dist",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/main.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE",
                "OPTIONS"
            ]
        }
    ]
}